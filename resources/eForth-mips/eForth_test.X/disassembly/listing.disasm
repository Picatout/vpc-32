940:                      * Example:         DmaSfmSetSeed(0xffffffff);
941:                      ********************************************************************/
942:                     extern __inline__ void __attribute__((always_inline)) DmaSfmSetSeed(unsigned int seed)
943:                     {
944:                         DCRCDATA=seed;
945:                     }
946:                     #define     DmaCrcSetSeed   DmaSfmSetSeed       // PIC32_3xx backward compatibility name
